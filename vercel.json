{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_SITE_URL": "https://myportfolio.mysticdatanode.net",
    "NEXT_PUBLIC_API_BASE_URL": "https://apiportfolio.mysticdatanode.net/api"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NEXT_PUBLIC_SITE_URL": "https://myportfolio.mysticdatanode.net",
      "NEXT_PUBLIC_API_BASE_URL": "https://apiportfolio.mysticdatanode.net/api"
    }
  },
  "regions": ["iad1"]
}