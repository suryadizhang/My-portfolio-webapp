{
  "name": "portfolio-monorepo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@11.4.2",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:web": "npm run dev --workspace=apps/web",
    "dev:api": "cd apps/service-python && uvicorn app.main:app --reload --port 8000",
    "build:rag": "cd apps/service-python && python scripts/build_rag_index.py",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "test:python": "cd apps/service-python && pytest -q",
    "clean": "turbo clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start": "node server.js",
    "start:production": "NODE_ENV=production node server.js"
  },
  "devDependencies": {
    "@turbo/gen": "^1.12.4",
    "prettier": "^3.2.5",
    "turbo": "^1.12.4",
    "typescript": "^5.0.0",
    "tsup": "^8.3.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "overrides": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/typography": "^0.5.16",
    "@types/nodemailer": "^7.0.1",
    "@vercel/og": "^0.8.5",
    "nodemailer": "^7.0.6",
    "resend": "^6.1.0"
  }
}
